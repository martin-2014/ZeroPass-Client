{
    "name": "zeropass",
    "version": "1.0.0",
    "main": "main.js",
    "author": "zpassapp.com",
    "private": true,
    "description": "An out-of-box UI solution for enterprise applications",
    "repository": "https://github.com/electron/electron-quick-start",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "deploy": "npm run build && npm run gh-pages",
        "dev": "npm run start:dev",
        "gh-pages": "gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "postinstall": "umi g tmp",
        "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "openapi": "umi openapi",
        "precommit": "lint-staged",
        "prettier": "prettier -c --write \"src/**/*\"",
        "serve": "umi-serve",
        "start": "cross-env UMI_ENV=dev BASE_URL=$npm_config_base_url PROXY_URL=$npm_config_proxy_url ETH_API_KEY=$npm_config_eth_api_key BSC_API_KEY=$npm_config_bsc_api_key CRYPTOCOMPARE_API_KEY=$npm_config_cryptocompare_api_key umi dev ",
        "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
        "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
        "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
        "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
        "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
        "pretest": "node ./tests/beforeTest",
        "test": "umi test",
        "test:all": "node ./tests/run-tests.js",
        "test:component": "umi test ./src/components",
        "tsc": "tsc --noEmit"
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/charts": "^1.3.3",
        "@ant-design/icons": "^4.5.0",
        "@ant-design/pro-descriptions": "^1.9.0",
        "@ant-design/pro-form": "^1.43.0",
        "@ant-design/pro-layout": "6.26.0",
        "@ant-design/pro-table": "^2.56.0",
        "@icon-park/react": "^1.3.5",
        "@umijs/route-utils": "^2.0.3",
        "antd": "^4.16.13",
        "antd-img-crop": "^4.2.3",
        "babel-plugin-import": "^1.13.5",
        "bip39": "^3.0.4",
        "browser-passworder": "^2.0.3",
        "classnames": "^2.2.6",
        "creditcards": "^4.2.0",
        "creditcards-types": "^3.2.1",
        "csv-parser": "^3.0.0",
        "csv-writer": "^1.6.0",
        "eth-balance-checker": "^0.2.0",
        "ethereumjs-wallet": "^1.0.2",
        "ethers": "^5.6.8",
        "generate-password": "^1.7.0",
        "idb": "^7.0.2",
        "javascript-time-ago": "^2.5.4",
        "lodash": "^4.17.11",
        "moment": "^2.25.3",
        "omit.js": "^2.0.2",
        "otpauth": "^7.1.2",
        "owasp-password-strength-test": "^1.3.0",
        "rc-menu": "^9.0.13",
        "rc-util": "^5.14.0",
        "react": "^17.0.0",
        "react-dev-inspector": "^1.1.1",
        "react-dom": "^17.0.0",
        "react-helmet-async": "^1.0.4",
        "secure-remote-password": "https://github.com/LinusU/secure-remote-password.git#73e5f732b6ca0cdbdc19da1a0c5f48cdbad2cbf0",
        "ua-parser-js": "^0.7.28",
        "umi": "^3.5.0",
        "umi-serve": "^1.9.10",
        "xlsx": "^0.18.2"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "^2.0.2",
        "@types/express": "^4.17.0",
        "@types/history": "^4.7.2",
        "@types/jest": "^26.0.0",
        "@types/lodash": "^4.14.144",
        "@types/owasp-password-strength-test": "^1.3.0",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-helmet": "^6.1.0",
        "@types/uuid": "^8.3.4",
        "@umijs/fabric": "^2.6.2",
        "@umijs/openapi": "^1.3.0",
        "@umijs/plugin-blocks": "^2.0.5",
        "@umijs/plugin-esbuild": "^1.0.1",
        "@umijs/preset-ant-design-pro": "^1.2.0",
        "@umijs/preset-dumi": "^1.1.7",
        "@umijs/preset-react": "^1.8.17",
        "@umijs/yorkie": "^2.0.3",
        "carlo": "^0.9.46",
        "cross-env": "^7.0.0",
        "cross-port-killer": "^1.1.1",
        "detect-installer": "^1.0.1",
        "enzyme": "^3.11.0",
        "eslint": "^7.1.0",
        "express": "^4.17.1",
        "file-loader": "^6.2.0",
        "gh-pages": "^3.0.0",
        "jsdom-global": "^3.0.2",
        "lint-staged": "^10.0.0",
        "mockjs": "^1.0.1-beta3",
        "prettier": "^2.3.2",
        "puppeteer-core": "^8.0.0",
        "stylelint": "^13.0.0",
        "typescript": "^4.2.2"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "gitHooks": {
        "commit-msg": "fabric verify-commit"
    }
}
